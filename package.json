    "babel-plugin-transform-react-inline-elements": "~6.8.0",
    "babel-polyfill": "~6.9.1",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-0": "~6.5.0",
    "classnames": "~2.2.5",
    "cross-env": "~1.0.8",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "enzyme": "~2.3.0",
    "eslint": "2.13.1",
