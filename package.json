    "json-loader": "~0.5.4",
    "karma": "~1.1.0",
    "karma-chrome-launcher": "~1.0.1",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "minx": "r-park/minx.git",
    "morgan": "~1.7.0",
