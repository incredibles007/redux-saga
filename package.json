{
  "name": "todo-redux-saga",
  "version": "0.0.0",
  "description": "Todo app with React, Redux, Redux-Saga, and Firebase",
  "homepage": "https://github.com/r-park/todo-redux-saga",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-park/todo-redux-saga.git"
